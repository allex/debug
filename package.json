{
  "name": "debug",
  "version": "4.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/visionmedia/debug.git"
  },
  "description": "small debugging utility",
  "keywords": [
    "debug",
    "log",
    "debugger"
  ],
  "files": [
    "src",
    "dist/debug.js",
    "dist/debug.min.js",
    "dist/debug.esm.js",
    "LICENSE",
    "README.md"
  ],
  "author": "TJ Holowaychuk <tj@vision-media.ca>",
  "contributors": [
    "Nathan Rajlich <nathan@tootallnate.net> (http://n8.io)",
    "Andrew Rhyne <rhyneandrew@gmail.com>",
    "Allex Wang <allex.wxn@gmail.com> (http://iallex.com)"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "xo",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "istanbul cover _mocha -- test.js",
    "posttest:node": "cat ./coverage/lcov.info | coveralls",
    "pretest:browser": "npm run build",
    "test:browser": "karma start --single-run",
    "build:debug": "rollup-worker -c",
    "build:test": "babel -d dist test.js",
    "build": "npm run build:debug && npm run build:test",
    "clean": "rimraf dist coverage"
  },
  "dependencies": {
    "ms": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "rimraf": "^2.5.4",
    "rollup-plugin-babel": "beta",
    "rollup-worker": "next",
    "xo": "^0.23.0"
  },
  "main": "./src/index.js",
  "browser": "./dist/debug.js",
  "unpkg": "./dist/debug.min.js",
  "module": "./dist/debug.esm.js"
}
